{
    "name": "xpp",
    "displayName": "ODE File Syntax Highlighter for XPP/XPPAUT",
    "description": "ODE files to be used with XPP/XPPAUT",
    "version": "0.0.1",
    "publisher": "wjmccann",
    "icon": "./images/icon.png",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xpp",
            "aliases": ["XPP ODE", "XPP", "XPPAUT"],
            "extensions": [".ode"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "xpp",
            "scopeName": "source.xpp",
            "path": "./syntaxes/xpp.tmLanguage.json"
        }]
    }
}